<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Frequency">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The number of oscillations or cycles per unit time of a wave, often measured in hertz (Hz).</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Wavelength">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The distance between successive crests of a wave, typically measured in nanometers (nm) for light waves.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Energy Change (ΔE)">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The difference in energy between two energy levels in an atom, associated with electron transitions.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Principal Quantum Number">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:disjointWith rdf:resource="http://www.test.org/chem_ontologies/classes/One-Electron Species"/>
  <owl:disjointWith rdf:resource="http://www.test.org/chem_ontologies/classes/Spectrum"/>
