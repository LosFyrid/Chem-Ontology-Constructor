  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/object_properties/has_principal_quantum_number"/>
      <owl:someValuesFrom rdf:resource="http://www.test.org/chem_ontologies/classes/Principal Quantum Number"/>
