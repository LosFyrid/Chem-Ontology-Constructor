<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.test.org/chem_ontologies/chem_ontology.owl"
         xmlns="http://www.test.org/chem_ontologies/chem_ontology.owl#"
         xmlns:meta="http://www.test.org/chem_ontologies/meta/">

<owl:Ontology rdf:about="http://www.test.org/chem_ontologies/chem_ontology.owl"/>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_element">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An electron configuration corresponds to a specific element.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_configuration_of">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration is a property of the atom, indicating how electrons are distributed among orbitals.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_electron_distribution">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration describes how electrons are distributed among orbitals, principal shells, and subshells.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_correlated_with">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration of elements is correlated with their physical and chemical properties.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_determined_by">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wave mechanics helps determine the electron configurations of elements.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_emission_affected_by_wavelength">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The wavelength of light affects the emission of electrons from hydrogen atoms.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/emits_electrons">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hydrogen atoms emit electrons when exposed to light of sufficient energy.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/facilitates_electron_emission">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hydrogen gas facilitates the emission of electrons when exposed to light.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_caused_by">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The quantization of the wave's energy is caused by the standing wave condition.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_explained_by">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The zero-point energy is explained by the uncertainty principle.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_prevented_from_resting_by">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The zero-point energy prevents the particle from being at rest, consistent with the uncertainty principle.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_in_energy_level">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An electron is in a specific energy level within a one-dimensional box.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_probability_in_region">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The probability of finding an electron in a specific region of a one-dimensional box.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_energy_level">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The hydrogen atom has quantized energy levels, which are integer values corresponding to quantum states.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_quantized_as">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Energy levels are quantized as quantum states, representing the possible states an electron can occupy in an atom.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_used_to_calculate_frequency">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Balmer Equation is used to calculate the frequency of spectral lines in the Balmer Series.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_used_to_calculate_wavelength">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Balmer Equation is used to calculate the wavelength of spectral lines in the Balmer Series.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_resulting_energy_change">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Transitions in a Hydrogen Atom result in an Energy Change (ΔE).</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_produced_spectral_line_frequency">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Transitions in a Hydrogen Atom produce spectral lines with specific frequencies.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_produced_spectral_line_wavelength">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Transitions in a Hydrogen Atom produce spectral lines with specific wavelengths.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_principal_quantum_number">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The spectrum is characterized by specific principal quantum numbers corresponding to the lines labeled A and B.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_exhibited_by">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The spectrum is exhibited by the one-electron species.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_outer_shell_electrons">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Configuration determines the number of outer-shell electrons in an atom.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_principal_electronic_shell">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Configuration determines the electrons in the principal electronic shell of an atom.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_unpaired_electrons">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Configuration determines the number of unpaired electrons in an atom.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/is_in_sixth_period">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Transition Elements can be found in the sixth period of the periodic table.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_probability_density">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The wave function has a probability density, which is the square of the wave function.</meta:information>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.test.org/chem_ontologies/object_properties/has_contribution_to_particle_behavior">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The wave function has a contribution to determining particle behavior by influencing the probability of finding a particle in a specific location.</meta:information>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Electron Configuration">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration is the distribution of electrons of an atom in atomic orbitals, specific to each element.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/distributionAmongOrbitals">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration of an atom describes how electrons are distributed among various orbitals in principal shells and subshells.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/correlationWithProperties">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration of elements can be correlated with their physical and chemical properties.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/determinationByWaveMechanics">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wave mechanics provides a set of rules to help write probable electron configurations for elements.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Wavelength">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The wavelength of light needed to eject electrons from hydrogen atoms is specified as 91.2 nm, with other experimental wavelengths being 80.0 nm and 70.0 nm.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Energy Levels">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The energy levels of a system are quantized, meaning they can only take on discrete values.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Kinetic Energy Increase">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The kinetic energy of a particle increases as the size of the box decreases.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Zero-Point Energy">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The lowest possible energy of a particle, which is not zero, indicating the particle cannot be at rest.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Fraction of Total Probability">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The fraction, as a percentage, of the total probability of finding an electron between points at 0 pm and 30 pm in a one-dimensional box 150 pm long, when the electron is in the n=5 energy level. This probability is a variable to be calculated based on the given conditions.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Quantized Energy Level">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The energy level of a hydrogen atom is quantized and corresponds to an integer value of n, indicating that n must be an integer for the energy level to be valid. The specific integer values are not provided in the text, but the condition of n being an integer is essential for the validity of the energy level.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Frequency of Radiation">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The frequency of the radiation corresponding to specific electron transitions in the Balmer series, determined using the Balmer equation.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Wavelength of Balmer Series Line">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The wavelength in nanometers of the line in the Balmer series corresponding to specific electron transitions, calculated using the Balmer equation.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Energy Change (ΔE) for Electron Transition">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The energy change associated with the transition of an electron between two energy levels in a hydrogen atom, calculated for transitions such as n=6 to n=3 and n=5 to n=2.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Frequency of Spectral Line">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The frequency of the spectral line produced by an electron transition in a hydrogen atom, calculated for specific transitions such as n=6 to n=3 and n=5 to n=2.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Upper Principal Quantum Number">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The upper principal quantum number corresponding to the spectral line labeled A or B, which is to be determined.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Lower Principal Quantum Number">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The lower principal quantum number corresponding to the spectral line labeled A or B, which is to be determined.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Number of Outer-Shell Electrons">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The number of electrons located in the outermost shell of an atom, which are involved in chemical bonding.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Number of Electrons in Principal Electronic Shell">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The number of electrons present in a specified principal electronic shell of an atom.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Number of Unpaired Electrons">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The number of electrons in an atom that are not paired with another electron in an orbital.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Transition Elements in Period">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Classification of elements as transition elements within a specified period of the periodic table.</meta:information>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="http://www.test.org/chem_ontologies/data_properties/Probability Density Value">
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The probability density is the square of the wave function, representing the likelihood of finding a particle in a specific location.</meta:information>
</owl:DatatypeProperty>

<owl:AnnotationProperty rdf:about="http://www.test.org/chem_ontologies/meta/embedding">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="http://www.test.org/chem_ontologies/meta/location">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="http://www.test.org/chem_ontologies/meta/information">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="http://www.test.org/chem_ontologies/meta/hierarchy_information">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:AnnotationProperty>

