</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Lanthanides">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lanthanides are a series of elements in the f-block of the periodic table, known for their rare earth properties.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Atom">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An atom is the basic unit of a chemical element, consisting of a nucleus and electrons distributed in orbitals.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Electron">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/object_properties/is_in_energy_level"/>
