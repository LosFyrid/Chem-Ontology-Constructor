      <owl:someValuesFrom rdf:resource="http://www.test.org/chem_ontologies/classes/Probability"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electrons are subatomic particles with a negative charge, distributed among orbitals in an atom.</meta:information>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An electron is a subatomic particle with a negative electric charge, commonly found in atoms and involved in chemical reactions.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Orbital">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Subshell"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Orbitals are regions around the nucleus of an atom where electrons are likely to be found.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Orbitals are regions around the nucleus of an atom where electrons are likely to be found, and they are part of Subshells.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Principal Shell">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Atom"/>
