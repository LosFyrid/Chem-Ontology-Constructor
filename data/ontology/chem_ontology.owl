    </owl:Restriction>
  </rdfs:subClassOf>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Elements found in the d-block of the periodic table, known for their ability to form variable oxidation states and colored compounds.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Transition Elements are a specific category within the Periodic Table.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Sixth Period">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Periodic Table"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The horizontal row in the periodic table that contains elements with electrons filling the sixth principal energy level.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sixth Period is a specific row within the Periodic Table.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Sh">
