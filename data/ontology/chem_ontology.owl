  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/object_properties/is_exhibited_by"/>
      <owl:someValuesFrom rdf:resource="http://www.test.org/chem_ontologies/classes/One-Electron Species"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A spectrum is a range of wavelengths of electromagnetic radiation emitted or absorbed by a substance.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Periodic Table">
