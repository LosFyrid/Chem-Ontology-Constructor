  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electrons located in the outermost shell of an atom, which are involved in chemical bonding.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Outer-Shell Electrons are a specific aspect of Electron Configuration.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Principal Electronic Shell">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Electron Configuration"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The main energy levels in an atom where electrons reside, denoted by the principal quantum number.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Principal Electronic Shell is a specific aspect of Electron Configuration.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Unpaired Electrons">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Electron Configuration"/>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electrons in an atom that are not paired with another electron in an orbital, often contributing to magnetic properties.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unpaired Electrons are a specific characteristic of electron arrangements.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Transition Elements">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Periodic Table"/>
