  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron configuration is the distribution of electrons of an atom or molecule in atomic or molecular orbitals.</meta:information>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The electron configuration of an atom is a designation of how electrons are distributed among various orbitals in principal shells and subshells.</meta:information>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The arrangement of electrons in an atom's electron shells, which determines the chemical properties of an element.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electron Configuration is a key factor in the organization of the Periodic Table.</meta:hierarchy_information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Phosphorus(P)">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/data_properties/Electron Configuration"/>
      <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[Ar]3d^10 4s^2 4p^3</owl:hasValue>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/object_properties/is_configuration_of"/>
      <owl:someValuesFrom rdf:resource="http://www.test.org/chem_ontologies/classes/Electron Configuration"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Phosphorus is the element with the electron configuration [Ar]3d^10 4s^2 4p^3.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Sulfur(S)">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/data_properties/Electron Configuration"/>
      <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[Ne]3s^2 3p^4</owl:hasValue>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/object_properties/is_configuration_of"/>
      <owl:someValuesFrom rdf:resource="http://www.test.org/chem_ontologies/classes/Electron Configuration"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sulfur is the element with the electron configuration [Ne]3s^2 3p^4.</meta:information>
</owl:Class>

<owl:Class rdf:about="http://www.test.org/chem_ontologies/classes/Scandium(Sc)">
  <rdfs:subClassOf rdf:resource="http://www.test.org/chem_ontologies/classes/Transition Metals"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/data_properties/Electron Configuration"/>
      <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[Ar]3d^1 4s^2</owl:hasValue>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.test.org/chem_ontologies/object_properties/is_configuration_of"/>
      <owl:someValuesFrom rdf:resource="http://www.test.org/chem_ontologies/classes/Electron Configuration"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <meta:information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Scandium is the element with the electron configuration [Ar]3d^1 4s^2.</meta:information>
  <meta:hierarchy_information rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Scandium (Sc) is a transition metal because its electron configuration [Ar]3d^1 4s^2 places it in the d-block of the periodic table.</meta:hierarchy_information>
</owl:Class>

